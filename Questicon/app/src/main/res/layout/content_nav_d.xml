<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.notexample.austin.questicon.NavD"
    tools:showIn="@layout/app_bar_nav_d">


    <FrameLayout
        android:id="@+id/framecharsearch"
        android:layout_marginTop="10dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#57F0F8FF">

            <ImageView
                android:layout_marginLeft="5dp"
                android:layout_marginTop="5dp"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:background="@mipmap/ic_viewchar"
                android:id="@+id/imageView5" />


            <TextView
                android:layout_marginTop="10dp"
                android:textColor="#F0F8FF"
                android:onClick="clickingCharSearch"
                android:id="@+id/charactersearch"
                android:layout_width="260dp"
                android:layout_height="50dp"
                android:textSize="18sp"
                android:text="Your character search"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true" />


        </RelativeLayout>
    </FrameLayout>
    <FrameLayout
        android:id="@+id/framecharacters"
        android:layout_below="@id/framecharsearch"
        android:layout_marginTop="10dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#57F0F8FF">

            <ImageView
                android:layout_marginLeft="5dp"
                android:layout_marginTop="5dp"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:background="@mipmap/ic_charactercho"
                android:id="@+id/imageView6" />


            <TextView
                android:layout_marginTop="10dp"
                android:textColor="#F0F8FF"
                android:onClick="clickingCharInfo"
                android:id="@+id/characterinfo"
                android:layout_width="260dp"
                android:layout_height="50dp"
                android:textSize="18sp"
                android:text="Characters"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true" />


        </RelativeLayout>
    </FrameLayout>

    <FrameLayout
        android:id="@+id/framebosses"
        android:layout_below="@id/framecharacters"
        android:layout_marginTop="10dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#57F0F8FF">

            <ImageView
                android:layout_marginLeft="5dp"
                android:layout_marginTop="5dp"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:background="@mipmap/ic_bosses"
                android:id="@+id/imageView9" />


            <TextView
                android:layout_marginTop="10dp"
                android:textColor="#F0F8FF"
                android:onClick="clickingBossesAc"
                android:id="@+id/bossestextview"
                android:layout_width="260dp"
                android:layout_height="50dp"
                android:textSize="18sp"
                android:text="Bosses"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true" />


        </RelativeLayout>
    </FrameLayout>

    <FrameLayout
        android:id="@+id/framedungeons"
        android:layout_below="@id/framebosses"
        android:layout_marginTop="10dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#57F0F8FF">

